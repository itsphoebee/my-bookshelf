<h1>All Books</h1>
<% if current_user.admin? %>
  <%= link_to 'New Book', new_book_path(@book), class: "btn btn-primary"%>
<% end %>

    <ul>
      <% @books.each do |b| %>
        <li>
          <div id ="booktitle">
          <%= link_to b.title, book_path(b) %>
        </div>
        by <%= b.author.name %></li>
      <% end %>
    </ul>
